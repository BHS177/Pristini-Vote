{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/admin",
      "dest": "api/index.js"
    },
    {
      "src": "/styles.css",
      "dest": "public/styles.css"
    },
    {
      "src": "/app.js",
      "dest": "public/app.js"
    },
    {
      "src": "/pristini.jpeg",
      "dest": "public/pristini.jpeg"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}

