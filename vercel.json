{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/admin",
      "dest": "api/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|jpeg|jpg|png|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}

